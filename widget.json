{
  "name": "Data interactive icon",
  "package": "com.fliplet.social-icons",
  "version": "1.0.0",
  "icon": "img/icon.png",
  "tags": ["type:component", "category:general", "beta:true"],
  "provider_only": false,
  "references": [],
  "settings": {
    "asyncRendering": true,
    "interfaceWidgetPackage": "com.fliplet.helper-configuration"
  },
  "html_tag": "div",
  "interface": {
    "dependencies": [
      "fliplet-core",
      "fliplet-studio-ui"
    ],
    "assets": [
      "js/interface.js"
    ]
  },
  "build": {
    "dependencies": [
      "jquery",
      "bootstrap",
      "fliplet-core",
      "fliplet-helper",
      "fliplet-datasources"
    ],
    "assets": [
      "css/build.css",
      "js/build.js",
      "css/index.scss"
    ],
    "appAssets": []
  },
  "themeSettings": {
    "variablePrefix": "social",
    "variables": [
      {
        "description": "Color",
        "fields": [
          {
            "name": "background",
            "default": "#ccc",
            "label": "Full icon color",
            "type": "color"
          }
        ]
      }
    ]
  }
}
